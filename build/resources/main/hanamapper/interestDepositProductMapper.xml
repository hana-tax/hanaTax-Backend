<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.domain.product.deposit.interest.mapper.InterestDepositProductMapper">

    <select id="getInterest" resultType="com.example.demo.domain.product.deposit.interest.dto.InterestDepositProductResponse">
        SELECT  depositId,
                interestId,
                duration,
                interestRate,
                createdAt,
                updatedAt
        FROM interest
        WHERE depositId = #{id}
    </select>


</mapper>